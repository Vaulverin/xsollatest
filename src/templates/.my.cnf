[client]
user = phalcon
password = secret
host = 127.0.0.1
port = 3306

[mysql]
user = phalcon
password = secret
host = 127.0.0.1
port = 3306
default-character-set=utf8
local-infile=1
show-warnings
auto-rehash
sigint-ignore
reconnect
pager  = grcat ~/.grcat

[mysqldump]
user = phalcon
password = secret
host = 127.0.0.1
port = 3306
